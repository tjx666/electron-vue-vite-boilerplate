{
  "groupRules": [
    ["^[@][^/]", {}],
    [
      "^@(/|$)",
      "^apis?(/|$)",
      "^assets(/|$)",
      "^components?(/|$)",
      "^typings?(/|$)",
      "^utils?(/|$)"
    ],
    { "flags": "named", "regex": "^[.]" },
    { "flags": "scripts", "regex": "[.]((css)|(less)|(scss))$" }
  ],
  "keepUnused": [".*"],
  "wrappingStyle": "prettier"
}
